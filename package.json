{
  "name": "preact-aria-widgets",
  "type": "module",
  "version": "0.0.1",
  "description": "A small set of Preact hooks related to creating ARIA-compliant widgets",
  "source": "src/index.tsx",
  "module": "/dist/index.js",
  "types": "/dist/index.d.ts",
  "packageManager": "pnpm@7.28.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -sc build:ts",
    "watch": "npx npm-run-all --npm-path npm -pc watch:*",
    "watch:ts": "tsc --watch",
    "build:ts": "tsc --build",
    "test": "jest",
    "clean": "tsc --build --clean"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "preact": "^10.13.0"
  },
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "lodash-es": "^4.17.21",
    "preact-prop-helpers": "github:mwszekely/preact-prop-helpers#nodenext"
  }
}
